className=\"check\" onClick={(e) => checkChild(`c-${category.sport_category_id}`)}>\n                                            <input\n                                                id={`cat-${category.sport_category_id}`}\n                                                className=\"sportFlag\"\n                                                type=\"checkbox\"\n                                            />\n                                            <span className=\"checkmark\" />\n                                        </div>\n                                        <label htmlFor={`cat-${category.sport_category_id}`} className=\"tournament-name\">{category.name}</label>\n                                    </div>\n                                </td>\n                                <td>\n                                    <div className=\"tournaments-group\" id={`c-${category.sport_category_id}`}>\n                                        {category.tournaments.map((tournament, t) =>\n                                            <div className=\"single-tournament\" key={`sport-t-${t}`}>\n                                                <div className=\"check\" >\n                                                    <input\n                                                        id={`t-${tournament.sport_tournament_id}`}\n                                                        className=\"sportFlag\"\n                                                        type=\"checkbox\"\n                                                        onChange={(e) => handleSingleClick(e)}\n                                                        value={tournament.sport_tournament_id}\n                                                    />\n                                                    <span className=\"checkmark\" />\n                                                </div>\n                                                <label htmlFor={`t-${tournament.sport_tournament_id}`} className=\"tournament-name\">{tournament.name}</label>\n                                            </div>\n                                        )}\n                                    </div>\n                                </td>\n                            </tr>\n                        )}\n                        </tbody>\n                        <tfoot>\n                        <tr>\n                            <td colSpan={2}>\n                                <div className=\"groupBtn\">\n                                    <a className=\"button\" onClick={() => print()}>Print</a>\n                                    <a className=\"button\" onClick=\"Go(0)\" title=\"\">Display</a>\n                                </div>\n                            </td>\n                        </tr>\n                        </tfoot>\n                    </table>\n\n                </div>\n            )}\n        </Fragment>\n    )\n}\n","/Users/max/Projects/sportsbook/betteck/betteck-web/src/Views/CMSPages.js",["535"],"import React from \"react\";\nimport useSWR from \"swr/esm/use-swr\";\n\nexport default function CMSPages({match}) {\n    const slug = match.params.slug;\n\n    const {data, error} = useSWR(`/utilities/cms/page/${slug}`);\n\n    return (\n        <>\n            <div id=\"MainContent\" className=\"sport\" style={{padding: '10px'}}>\n                <h2>{data?.title}</h2>\n                <div  dangerouslySetInnerHTML={{ __html: data?.body}} />\n            </div>\n        </>\n    )\n}\n","/Users/max/Projects/sportsbook/betteck/betteck-web/src/Views/Tipsters.js",["536"],"import React, { useEffect, useState} from \"react\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport { getTipsters} from \"../Services/apis\";\nimport {LOADING} from \"../Redux/types\";\nimport {NavLink} from \"react-router-dom\";\nimport {formatName} from \"../Utils/helpers\";\n\n\nexport default function Tipsters({location}) {\n\n    const [results, setResults] = useState